pipeline {
    agent any

    stages {
        stage('checkout') {
            steps {
                git branch: 'main', url: 'https://github.com/samishah6799/mlops-demo.git'
            }
        }
        stage('Deploy') {
            steps {
                script {
                    def myScript = load 'example.groovy'
                    //myScript.deploy("PROD")
                    def VARIABLEE = "HOJA BHAI"
                    myScript.build(VARIABLEE)
                }
            }
        }
    }
}
