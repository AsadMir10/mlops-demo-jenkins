
pipeline {
    agent any

    stages {
        stage('checkout') {
            steps {
                git branch: 'main', url: 'https://github.com/samishah6799/mlops-demo.git'
            }
        }
        stage('Build') {
            steps {
                script {
                    def data =  load 'example.groovy'
                    data.build()
                    // echo 'Building'
                    // sh 'pip3 install -r requirements.txt'
                }
            }
        }
    }
}
